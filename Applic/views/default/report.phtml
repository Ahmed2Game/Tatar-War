{function="load_lang('ui/report')"}
<h1>{#LANGUI_RPT_T1}</h1>
<div id="textmenu">
    <a href="report" {if="( $selectedTabIndex == 0 )"} class="selected" {/if} >{#LANGUI_RPT_T2}</a>
    |
    <a href="report?t=1" {if="( $selectedTabIndex == 1 )"} class="selected" {/if} >{#LANGUI_RPT_T3}</a>
    |
    <a href="report?t=2" {if="( $selectedTabIndex == 2 )"} class="selected" {/if} >{#LANGUI_RPT_T4}</a>
    |
    <a href="report?t=3" {if="( $selectedTabIndex == 3 )"}class="selected"{/if} >{#LANGUI_RPT_T5}</a>
    |
    <a href="report?t=4" {if="( $selectedTabIndex == 4 )"}class="selected"{/if} >{#LANGUI_RPT_T6}</a>
</div>
{if="( $showList )"}
<form method="post" action="report?p={$pageIndex}{if="( 0 < $selectedTabIndex )"}&t={$selectedTabIndex}{/if}" name="msg">
    <table cellpadding="1" cellspacing="1" id="overview" class="row_table_data">
        <thead>
            <tr>
                <th colspan="2">{#LANGUI_RPT_T7}</th>
                <th class="sent">{#LANGUI_RPT_T8}</th>
            </tr>
        </thead>
        <tbody>
            {loop="$dataListArray"}
                <tr>
                    <td class="sel">
                        <input class="check" type="checkbox" name="dr[]" value="{$value.id}">
                    </td>
                    <td class="sub">
                        <img src="{function="add_style('x.gif', ASSETS_DIR)"}" class="iReport iReport{$value._rptResultCss}" alt="{$value.btext}" title="{$value.btext}">
                        <div>
                            <a href="report?id={$value.id}">
                                {$value.rpt_cat}
                            </a>
                            {if="( !$value['is_readed'] )"}
                                 {#LANGUI_RPT_T9}
                            {/if}
                        </div>
                    </td>
                    <td class="dat">{$value.mdate}</td>
                </tr>
            {/loop}
            {if="( $counter1 == -1 )"}
                <tr>
                    <td colspan="3" class="none">{#LANGUI_RPT_T10}</td>
                </tr>
            {/if}
        </tbody>
        <tfoot>
            <tr>
                <th>
                    {if="$counter1 > -1"}
                    <input class="check"  type="checkbox" id="s10" name="s10" onclick="Allmsg(this.form);">{/if}</th>
                    <th class="buttons">
                    {if="$counter1 > -1"}
                        <input name="delmsg" value="{#LANGUI_RPT_T11}" type="image" id="btn_delete" class="dynamic_img" src="{function="add_style('x.gif', ASSETS_DIR)"}" alt="{#LANGUI_RPT_T11}">
                    {/if}
                    </th>
                    <th class="navi">
                        {$getPreviousLink}
                        {$getNextLink}
                    </th>
                </tr>
            </tfoot>
        </table>
</form>
{elseif="( $reportData['rpt_cat'] == 1 )"}
<table cellpadding="1" cellspacing="1" id="report_surround">
        <thead>
            <tr>
                <th>{#LANGUI_RPT_T7}:</th>
                <th>
                    {$getVillageName}
                    {$getreportactiontext}
                    {$getVillageName_to}
                </th>
            </tr>
            <tr>
                <td class="sent">{#LANGUI_RPT_T8}:</td>
                <td>
                    {#text_in_lang} {$reportData.mdate}
                    <span>{#time_hour_lang2} {$reportData.mtime}</span>
                </td>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td colspan="2" class="empty"></td>
            </tr>
            <tr>
                <td colspan="2" class="report_content">
                    <table cellpadding="1" cellspacing="1" id="trade">
                        <thead>
                            <tr>
                                <td>&nbsp;</td>
                                <td>
                                    {if="( 0 < intval( $reportData['from_player_id'] ) )"}
