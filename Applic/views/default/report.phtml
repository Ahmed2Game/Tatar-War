                                        {#LANGUI_RPT_T12}
                                        <a href="village3?id={$reportData.from_village_id}">
                                            {$getVillageName}
                                        </a>
                                        {else}
                                            <span class="none">{$reportData.from_player_name}</span>
                                            {#LANGUI_RPT_T12}
                                            <span class="none">[?]</span>
                                        {/if}
                                    </td>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <th>{#LANGUI_RPT_T13}</th>
                                    <td>
                                        <img class="r1" src="{function="add_style('x.gif', ASSETS_DIR)"}" alt="{#item_title_1}" title="{#item_title_1}">
                                        {$reportData.resources.0} |
                                        <img class="r2" src="{function="add_style('x.gif', ASSETS_DIR)"}" alt="{#item_title_2}" title="{#item_title_2}">
                                        {$reportData.resources.1} |
