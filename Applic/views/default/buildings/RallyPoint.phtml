{if="$war_to_village_size && $tap == 1"}
    <h4>
    {#LANGUI_CUSTBU_RP_t1}
    </h4>
    {loop="$war_to_village"}
        <table class="troop_details" cellpadding="1" cellspacing="1">
            <thead>
                <tr>
                    <td class="role">
                        {if="( $value1['actionRow'] != NULL && $value1['actionRow']['player_id'] == $value1['taskTable']['player_id'] )"}
                            <a href="village3?id={$value.taskTable.village_id}">{$value.action2}</a>
                        {else}
                            {$value.action2}
                        {/if}
                    </td>
                    <td colspan="{$value.colspan}">
                        <a href="village3?id={$value.taskTable.to_village_id}"><p>{$value.action1}</p></a>
                    </td>
                </tr>
            </thead>
            <tbody class="units">
                <tr>
                    <th>&nbsp;</th>
                    {loop="$value.troops"}
                        <td>
                            <img src="{function="add_style('x.gif', ASSETS_DIR)"}" class="unit u{$key2}" title="{function="constant('troop_'.$key2 )"}" alt="{function="constant('troop_'.$key2 )"}">
                        </td>
                    {/loop}
                    {if="($value1['hasHero'] && $value1['procType'] == QS_WAR_REINFORCE )"}
                        <td>
                            <img src="{function="add_style('x.gif', ASSETS_DIR)"}" class="unit uhero" title="{#troop_hero}" alt="{#troop_hero}">
                        </td>
                    {/if}
                    </tr>
                    <tr>
                        <th>{#LANGUI_CUSTBU_RP_t6}</th>
                        {loop="$value.troops"}
                            {if="$value1.procType != QS_WAR_REINFORCE && $artPower && $value2 > 0"}
                                <td>?</td>
                            {elseif="$value1.procType != QS_WAR_REINFORCE"}
                                <td class="none">?</td>
                            {elseif="( $value2 == 0 )"}
                                <td class="none">0</td>
                            {else}
                                <td>
                                    {$value2}
                                </td>
                            {/if}
                        {/loop}
                        {if="( $value1['hasHero'] && $value1['procType'] == QS_WAR_REINFORCE )"}
                            <td>1</td>
                        {/if}
            </tbody>
            <tbody class="infos">
                <tr>
                    <th>{#LANGUI_CUSTBU_RP_t7}</th>
                    <td colspan="{$value.colspan}">
                        <div class="in small">
                        {#text_in_lang}
                        <span id="timer1">
                            {$value.taskTable.remainingSeconds|secondstostring}
                        </span>
                        {#time_hour_lang}
                        </div>
                    </td>
                </tr>
            </tbody>
            {if="( $value['resources'] != NULL )"}
            <tbody class="infos">
                <tr>
                    <th>{#LANGUI_CUSTBU_RP_t8}</th>
                    <td colspan="{$value.colspan}">
                        <img class="r1" src="{function="add_style('x.gif', ASSETS_DIR)"}" alt="{#item_title_1}" title="{#item_title_1}">
                        {$value.resources.0}
                       |<img class="r2" src="{function="add_style('x.gif', ASSETS_DIR)"}" alt="{#item_title_2}" title="{#item_title_2}">
                        {$value.resources.1}
                       |<img class="r3" src="{function="add_style('x.gif', ASSETS_DIR)"}" alt="{#item_title_3}" title="{#item_title_3}">
                        {$value.resources.2}
                       |<img class="r4" src="{function="add_style('x.gif', ASSETS_DIR)"}" alt="{#item_title_4}" title="{#item_title_4}">
                        {$value.resources.3}
                    </td>
                </tr>
            </tbody>
            {/if}
        </table>
    {/loop}
{/if}


