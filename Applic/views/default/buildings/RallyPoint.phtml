                                <a href="profile?uid={$value.troopTable.villageData.player_id}">
                                {#LANGUI_CUSTBU_RP_t17}

                                {if="( 0 < intval( $value['troopTable']['villageData']['player_id'] ) )"}
                                    {$value.troopTable.villageData.player_name}
                                {else}
                                    <span class="none">[?]</span>
                                {/if}
                                </a>
                            </td>
                        </tr>
                    </thead>
                    <tbody class="units">
                        <tr>
                            <th>&nbsp;</th>
                                {loop="$value.troopTable.troops"}
                                    <td>
                                        <img src="{function="add_style('x.gif', ASSETS_DIR)"}" class="unit u{$key}" title="{function="constant('troop_'.$key )"}" alt="{function="constant('troop_'.$key )"}">
                                    </td>
                                {/loop}
                                {if="( $value['troopTable']['hasHero'] )"}
                                    <td>
                                        <img src="{function="add_style('x.gif', ASSETS_DIR)"}" class="unit uhero" title="{#troop_hero}" alt="{#troop_hero}">
                                    </td>
                                {/if}
                                </tr>
                                <tr>
                                    <th>{#LANGUI_CUSTBU_RP_t6}</th>
                                    {loop="$value.troopTable.troops"}
                                        {if="( $value == 0 )"}
                                            <td class="none">0</td>
                                        {else}
                                            <td>
                                                {$value}
                                            </td>
                                        {/if}
                                    {/loop}
                                {if="( $value['troopTable']['hasHero'] )"}
                                    <td>1</td>
                                {/if}
                                </tr>
                            </tbody>
                            <tbody class="infos">
                                <tr>
                                    <th>{#LANGUI_CUSTBU_RP_t18}</th>
                                    <td colspan="{$value.colspan}">
                                        <div class="sup">
                                            {$value.troopTable.cropConsumption}
                                            <img class="r4" src="{function="add_style('x.gif', ASSETS_DIR)"}" title="{#item_title_4}" alt="{#item_title_4}">
                                            {#LANGUI_CUSTBU_RP_t19}
                                        </div>
                                        <div class="gback">
                                            <a href="v2v?d1={$value.troopTable.villageData.id}&o={$key1}">
                                                {#LANGUI_CUSTBU_RP_t20}
                                            </a>
                                        </div>
                                </td>
                            </tr>
                        </tbody>
                </table>
            {/loop}
        {/if}
    {/loop}
{/if}